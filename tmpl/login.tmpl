<h2>Sign In</h2>
<form method="post">
<p>
	<tmpl_var name=error>
</p>
<table>
<tr>
	<td class="label">
			Username
		</td>
		<td>
			<input type="text" name="username" value="<tmpl_var name=username>">
		</td>
	</tr>

	<tr>
		<td class="label">
			Password
		</td>
		<td>
			<input type="password" name="password" value="">
		</td>
	</tr>
</table>

<input type="submit">
</form>
